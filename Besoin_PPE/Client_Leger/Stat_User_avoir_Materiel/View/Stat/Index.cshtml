@model IEnumerable<StockLibrary.Models.Stat>

@{
    ViewData["Title"] = "Stat";
}

<div class="" style="margin-right:400px">
    <h1 class="display-4 text-center">Stat</h1>

  

    <table class="table table-striped">

        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.LocationName)</th>
                <th>@Html.DisplayNameFor(model => model.AssetTypes)</th>
                <th>@Html.DisplayNameFor(model => model.NBMatriel)</th>

            </tr>
        </thead>

        <tbody>
            @{
                foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.LocationName)</td>
                        <td>@Html.DisplayFor(modelItem => item.AssetTypes)</td>
                        <td>@Html.DisplayFor(modelItem => item.NBMatriel)</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
